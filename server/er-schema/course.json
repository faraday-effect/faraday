{
  "entity": {
    "name": "Course",
    "pk": "id",
    "description": "Course",
    "attributes": [
      {
        "name": "number",
        "type": "string",
        "description": "Course number (e.g., `243`, `103H`)"
      },
      {
        "name": "title",
        "type": "string",
        "description": "Course title"
      }
    ]
  },
  "relationships": [
    {
      "name": "prefix",
      "to": "course_prefix",
      "type": "manyToOne",
      "description": "Course prefix for this course"
    },
    {
      "name": "department",
      "to": "department",
      "type": "manyToOne",
      "description": "Department offering this course"
    },
    {
      "name": "offerings",
      "to": "offering",
      "type": "oneToMany",
      "description": "Offerings of this course"
    }
  ]
}
