{
  "entity": {
    "name": "term",
    "description": "Academic term",
    "pk": "id",
    "attributes": [
      {
        "name": "name",
        "description": "Term name (e.g., 'Fall 2021')",
        "type": "string"
      },
      {
        "name": "startDate",
        "description": "Starting date of term",
        "type": "date"
      },
      {
        "name": "endDate",
        "description": "Ending date of term",
        "type": "date"
      }
    ]
  },
  "relationships": [
    {
      "name": "offerings",
      "to": "offering",
      "type": "oneToMany",
      "description": "Offerings of courses during this term"
    }
  ]
}
